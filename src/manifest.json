{
  "manifest_version": 3,
  "name": "Current Tabs",
  "version": "1.0.0",
  "description": "List, copy & save info of currently open tabs",
  "permissions": ["tabs"],
  "incognito": "spanning",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
